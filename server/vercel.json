{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/signup",
      "dest": "/index.js"
    },
    {
      "src": "/login",
      "dest": "/"
    },
    {
      "src": "/currentuserprofile",
      "dest": "/"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
